#!/usr/bin/env bash
# Root-level Gradle wrapper stub for CI environments expecting ./gradlew.
# This Expo-managed project doesn't include a native Android build by default.
# The stub exits successfully to prevent CI failures.
set -e
echo "Root-level gradlew stub invoked with args: $*"
echo "No native Android project is present. This is expected for Expo-managed workflow."
exit 0
